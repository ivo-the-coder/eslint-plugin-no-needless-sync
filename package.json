{
  "name": "eslint-plugin-no-needless-sync",
  "version": "1.1.2",
  "description": "A plugin to detect needlessly synchonized async function calls",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Ivo Genev",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint .",
    "test:unit": "jest \"^.*(lib).*.spec.js\"",
    "test:integration": "jest \"^.*(tests).*/integration.*\"",
    "format": "prettier -w ."
  },
  "dependencies": {
    "requireindex": "^1.2.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-eslint-plugin": "^4.0.1",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^27.5.1",
    "prettier": "2.8.3"
  },
  "engines": {
    "node": "12.x || 14.x || >= 16"
  },
  "peerDependencies": {
    "eslint": ">=6"
  },
  "license": "ISC"
}
